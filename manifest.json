{
    "name": "Loans satisfied",
    "version": "1.0",
    "manifest_version": 2,


    "description": "RTest",
    "icons": {
        "128": "100_percent_128.png"
    },

    "browser_action": {
        "default_title": "Test Extension",
        "default_popup": "popup.html"
    },

    "permissions": [
        "http://*/*", "https://*/*"
    ],
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "run_at": "document_idle",
          "js": ["popup.js"]
        }
    ]
}